body{background:#FAF7EB; /*background:#F3F3F3;*/ background:url(../img/Twi/Tile.gif) repeat center;}
div#Cont{/*background:#FFF; border:1px solid #EFECDE;*/} 
div#Cont:before,
div#Cont:after {
	-webkit-box-shadow: 0 8px 30px rgba(0, 0, 0, 0.9); -moz-box-shadow: 0 8px 30px rgba(0, 0, 0, 0.9); box-shadow: 0 8px 30px rgba(0, 0, 0, 0.9); 
}
/* - */
header{
		/*background:url(../img/Twi/Logo.jpg) no-repeat left -8px; */
	p{color:#003399;}
}
/* - */
.Prog{
	a{color:transparent; color:#99bad9\9;
			background: #99bad9;
			background: -moz-linear-gradient(top,  #99bad9 0%, #8aacd1 50%, #769bc5 100%);
			background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#99bad9), color-stop(50%,#8aacd1), color-stop(100%,#769bc5));
			background: -webkit-linear-gradient(top,  #99bad9 0%,#8aacd1 50%,#769bc5 100%);
			background: -o-linear-gradient(top,  #99bad9 0%,#8aacd1 50%,#769bc5 100%);
			background: -ms-linear-gradient(top,  #99bad9 0%,#8aacd1 50%,#769bc5 100%);
			background: linear-gradient(top,  #99bad9 0%,#8aacd1 50%,#769bc5 100%);
		&:focus, &:active, &:hover{color:transparent; color:#99bad9\9;}
		&.Visited{color:transparent; color:#d7d7d7\9;
				background: #d7d7d7;
				background: -moz-linear-gradient(top,  #d7d7d7 0%, #e3e3e3 67%, #e6e6e6 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#d7d7d7), color-stop(67%,#e3e3e3), color-stop(100%,#e6e6e6));
				background: -webkit-linear-gradient(top,  #d7d7d7 0%,#e3e3e3 67%,#e6e6e6 100%);
				background: -o-linear-gradient(top,  #d7d7d7 0%,#e3e3e3 67%,#e6e6e6 100%);
				background: -ms-linear-gradient(top,  #d7d7d7 0%,#e3e3e3 67%,#e6e6e6 100%);
				background: linear-gradient(top,  #d7d7d7 0%,#e3e3e3 67%,#e6e6e6 100%);
			&:focus, &:active, &:hover{color:#000;}
		}
		&.Current{color:#FFF;
				background: #376ecb;
				background: -moz-linear-gradient(top,  #376ecb 0%, #2053be 65%, #1945b2 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#376ecb), color-stop(65%,#2053be), color-stop(100%,#1945b2));
				background: -webkit-linear-gradient(top,  #376ecb 0%,#2053be 65%,#1945b2 100%);
				background: -o-linear-gradient(top,  #376ecb 0%,#2053be 65%,#1945b2 100%);
				background: -ms-linear-gradient(top,  #376ecb 0%,#2053be 65%,#1945b2 100%);
				background: linear-gradient(top,  #376ecb 0%,#2053be 65%,#1945b2 100%);
			&:focus, &:active, &:hover{color:#FFF;}
		}
	}
}
/* - */
#Main{
	h2{color:#000; border-bottom:2px solid #789dc6;}
	
	.spotlight, .spotlight_para{border-top:2px solid #000; border-bottom:1px solid #000; background:#F2DA6D;
			background: #F2D13D;
			background: #f0e096;
			background: -moz-radial-gradient(center, ellipse cover,  #f0e096 0%, #f2d13d 100%);
			background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#f0e096), color-stop(100%,#f2d13d));
			background: -webkit-radial-gradient(center, ellipse cover,  #f0e096 0%,#f2d13d 100%);
			background: -o-radial-gradient(center, ellipse cover,  #f0e096 0%,#f2d13d 100%);
			background: -ms-radial-gradient(center, ellipse cover,  #f0e096 0%,#f2d13d 100%);
			background: radial-gradient(center, ellipse cover,  #f0e096 0%,#f2d13d 100%);
			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f0e096', endColorstr='#f2d13d',GradientType=1 );
		li:before{border-color:transparent #000;}
	}
	
	
	/* sections */
	section.sequence{
		ul.Nav{	background: #dedede;
				background: -moz-linear-gradient(top,  #dedede 0%, #d7d7d7 52%, #d1cfd0 76%, #cdcdcd 94%, #000000 95%, #000000 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#dedede), color-stop(52%,#d7d7d7), color-stop(76%,#d1cfd0), color-stop(94%,#cdcdcd), color-stop(95%,#000000), color-stop(100%,#000000));
				background: -webkit-linear-gradient(top,  #dedede 0%,#d7d7d7 52%,#d1cfd0 76%,#cdcdcd 94%,#000000 95%,#000000 100%);
				background: -o-linear-gradient(top,  #dedede 0%,#d7d7d7 52%,#d1cfd0 76%,#cdcdcd 94%,#000000 95%,#000000 100%);
				background: -ms-linear-gradient(top,  #dedede 0%,#d7d7d7 52%,#d1cfd0 76%,#cdcdcd 94%,#000000 95%,#000000 100%);
				background: linear-gradient(top,  #dedede 0%,#d7d7d7 52%,#d1cfd0 76%,#cdcdcd 94%,#000000 95%,#000000 100%);
			a{
					background:#FFF; color:#002BA8; box-shadow: 0 5px 5px rgba(0, 0, 0, 0.8);
				&:hover, &:focus, &:active{
						background:#F2D13D; color:#000; @include box-shadow(rgba(0, 0, 0, 0.9) 0 5px 5px);
				}
				&.Selected{
						background:#002BA8; color:#FFF; @include box-shadow(rgba(0, 0, 0, 0.8) 0 7px 7px); border:0;
				}
			}
		}
		ul.Options{
			a{background:#4a4a4a; color:#FFF;
				&:before{background:#00268f;}
				&:after{}
				&.Reset{
					&:after{background:url(../img/Options.png) no-repeat left top;}
				}
				&.Check{
					&:after{background:url(../img/Options.png) no-repeat -43px top;}
				}
				&:hover, &:focus, &:active{background:#313131;
					&:before{background:#00236e;}
				}
				&.Disabled{background:#4a4a4a; color:#FFF;
					&:before{background:#00268f;}
					&:hover, &:focus, &:active{background:#4a4a4a; color:#FFF;
						&:before{background:#00268f;}
					}
				}
			}
		}
	}
	
	section.mcq{
			@include box-shadow(rgba(0, 0, 0, 0.2) 0 0 15px);
		legend{
				@include box-shadow(rgba(0, 0, 0, 0.2) 0 5px 15px);	color:#FFF; text-shadow: 0 1px 1px #000;
				background: #000;
				background: -moz-linear-gradient(top,  #3a3a3a 0%, #393939 2%, #858585 3%, #686868 4%, #565656 5%, #4f4f4f 6%, #3e3e3e 13%, #313131 20%, #292929 28%, #171717 58%, #0d0d0d 79%, #070707 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#3a3a3a), color-stop(2%,#393939), color-stop(3%,#858585), color-stop(4%,#686868), color-stop(5%,#565656), color-stop(6%,#4f4f4f), color-stop(13%,#3e3e3e), color-stop(20%,#313131), color-stop(28%,#292929), color-stop(58%,#171717), color-stop(79%,#0d0d0d), color-stop(100%,#070707));
				background: -webkit-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
				background: -o-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
				background: -ms-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
				background: linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3a3a3a', endColorstr='#070707',GradientType=0 );
			&:before{
			   border-color:transparent #000;
			}
			&:after{
					background:#002BA8; color:#FFF; border:6px solid #121212; text-shadow: 0 0 0 #956200;
			}
		}
		ul{
			li{
				label{color:#000;
					&:hover{background:#E5E5E5;}
					&.Selected{background:#C9DEFF;}
				}
			}
		}
	}
	
	.token_bank{
		ul{
			li{
				background-color:#E2E2E2; background-color:rgba(212, 212, 212, 0.7);
				span.token{
						background:#002BA8; color:#FFF; @include box-shadow(rgba(0, 0, 0, 0.3) 0 3px 3px); border:2px solid #002BA8; text-shadow: 0 1px 1px #3C0027;
					&:hover{
						background:#C9DEFF; color:#000; @include box-shadow(rgba(0, 0, 0, 0.2) 0 3px 3px); border:2px solid #002BA8; text-shadow: 0 0 0 #002BA8;
					}
				} 
			}
		}
	}

	.dnd_columns{
		.column{
				background-color:#EAEAEA;
			h2{		color:#FFF; text-shadow: 0 1px 1px #000;
					background: #000;
					background: -moz-linear-gradient(top,  #3a3a3a 0%, #393939 2%, #858585 3%, #686868 4%, #565656 5%, #4f4f4f 6%, #3e3e3e 13%, #313131 20%, #292929 28%, #171717 58%, #0d0d0d 79%, #070707 100%);
					background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#3a3a3a), color-stop(2%,#393939), color-stop(3%,#858585), color-stop(4%,#686868), color-stop(5%,#565656), color-stop(6%,#4f4f4f), color-stop(13%,#3e3e3e), color-stop(20%,#313131), color-stop(28%,#292929), color-stop(58%,#171717), color-stop(79%,#0d0d0d), color-stop(100%,#070707));
					background: -webkit-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
					background: -o-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
					background: -ms-linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
					background: linear-gradient(top,  #3a3a3a 0%,#393939 2%,#858585 3%,#686868 4%,#565656 5%,#4f4f4f 6%,#3e3e3e 13%,#313131 20%,#292929 28%,#171717 58%,#0d0d0d 79%,#070707 100%);
					filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3a3a3a', endColorstr='#070707',GradientType=0 );
			}
			span.token{
					background:#002BA8; color:#FFF; border:2px solid #002BA8; text-shadow: 0 1px 1px #3C0027;
				&:hover{
					background:#C9DEFF; color:#000; @include box-shadow(rgba(0, 0, 0, 0.2) 0 3px 3px); border:2px solid #002BA8; text-shadow: 0 0 0 #002BA8;
				}
			}
			.dropIndicator{
					background-color:#E2E2E2; background-color:rgba(212, 212, 212, 0.7);
			}
		}
	}
	
	ul.hotspot{
		li{
			div.Canvas{
				div.Overlay{
					div.Spot{background:transparent; border:10px solid #FFF; background-color:rgba(255, 255, 255, 0.5) !important; @include box-shadow(rgba(0, 0, 0, 0.4) 0 0 3px);
						&:hover, &:focus, &:active{
								border:10px solid #000; @include box-shadow(rgba(0, 0, 0, 0.8) 0 0 4px);
						}					 
					}	
				}
			}
		}		
	}	
}
/* - */
.SpotInfo{
		background:#F9F9F9; border-top:2px solid #003399; border-bottom:2px solid #E6BC00;
	h2{color:#000;}
}
.FeedBack{border:1px solid #FFF;  background:#FFF;
	.FeedMsg{background:#FFF; border-top:2px solid #003399; border-bottom:2px solid #E6BC00;}
	h2{color:#000;}
}
/* - */
footer{
	li a{
		&.Prev{
				background:url(../img/Twi/NextPrev.png) no-repeat center -100px;
				background-color:#003399; background-color:rgba(1, 51, 153, 0.9);
		}
		&.Next{
				background:url(../img/Twi/NextPrev.png) no-repeat center top;
				background-color:#003399; background-color:rgba(1, 51, 153, 0.9);
		}
	}
}




/* - MCQ - */
#Main{
	.exercise{margin:0; padding:0; list-style:none;
		li{margin:0; padding:0; list-style:none;}
	}
	.sequenced{
		.mcq{@include box-shadow(#d8d8d8 0 0 0 0 inset);

			p{@include box-shadow(rgba(0, 0, 0, 0) 0 0 0); color:#FFF; text-shadow: 0 1px 1px #000; background:#4a4a4a;
				&:before{border-color:transparent #4a4a4a;}
				&:after{background:#1643a0; color:#FFF; border:6px solid #4a4a4a; text-shadow: 0 0 0 #333;}
			}
			ul.Answers{@include box-shadow(#9f9ea3 0 0 0 2px inset); background:#f5f5f5;
				li{
					label{color:#000;
						&:hover{background:#d4e1ee;}
						&.Selected{background:#2052bd; color:#FFF;} 
					}
				}
			}
					
		}
	}
}

/* ScoreCard */
#Main{
	.score_card{background:#333333; color:#fdfdfd;
		&:before{ border-color:transparent #333; }
		h2{color:#f5dd6f; border-bottom:1px solid #707070;}
		p.Score{color:#FFF; border-bottom:1px solid #707070; border-left:1px solid #707070;}
		p.ScoreDetail{color:#FFF; border-bottom:1px solid #707070; border-left:1px solid #707070; /*border-right:1px solid #707070;*/
			span{color:#f5dd6f; font-weight:bold;}
		}
		p.FeedbackMsg{border-bottom:1px solid #707070; border-left:1px solid #707070; border-top:1px solid #707070;}
	}
}

#Main{
	.score_card{background:#f3f3f3; color:#333; /*border:1px solid #dce3f1;*/
		&:before{ border-color:transparent #f3f3f3;}
		h2{color:#000; border-bottom:1px solid #d4e1ee;}
		p.Score{color:#333; border-bottom:1px solid #d4e1ee; border-left:1px solid #d4e1ee;}
		p.ScoreDetail{color:#333; border-bottom:1px solid #d4e1ee; border-left:1px solid #d4e1ee;
			span{color:#000; font-weight:bold;}
		}
		p.FeedbackMsg{border-bottom:1px solid #d4e1ee; border-left:1px solid #d4e1ee; border-top:1px solid #d4e1ee;}
	}
}


/* - TextCompare */
#Main{
	.text_compare{
		textarea{border:1px solid #b7b7b7; color:#000;}
		.Col.Answer{background:#333333; @include box-shadow(#9f9ea3 0 0 0 2px inset); background:#f5f5f5;
			h2{background:#000; color:#FFF; border:0;
				&:after{background:url(../img/Twi/DwnArrow.png) no-repeat center center;}
			}
			.ScrollArea{margin-right:2px; color:#000;} 
		}
		input{color:#FFF; background:#4a4a4a; border:0;
			&:hover{background:#313131;}
		}
	}
}



/*	Tablet Layout: landscape	*/
@media only screen and (min-width:945px) and (max-width: 1024px) { 
	footer{
		li a{
			&.Prev, &.Next{
				background-color:rgba(0, 51, 153, 0.3);
			}
			&:focus, &:active, &:hover{
				background-color:rgba(0, 51, 153, 0.3);
			}
		}
	}
}

@media only screen and (min-width: 768px) and (max-width: 945px) {
	footer{
		li a{
			&.Prev, &.Next{
				background-color:rgba(0, 51, 153, 0.3);
			}
			&:focus, &:active, &:hover{
				background-color:rgba(0, 51, 153, 0.3);
			}
		}
	}
 }

/*		Mobile Layout */
@media only screen and (max-width: 737px) {
	/* - Splash - */
	html.FrontPage{
		div.Outro{
			/*background-size:37px 29px;*/ background-size:200px 29px; background-position:10px 7px;
		}
	}
	footer{
		li a{
			&.Prev{
					background:#000 url(../img/NextPrevMob.png) no-repeat center -45px;
			}
			&.Next{
					background:#000 url(../img/NextPrevMob.png) no-repeat center top;
			}
		}
	}
}

